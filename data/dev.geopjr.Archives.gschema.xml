    <key name="singlefile-insert-meta-csp" type="b">
      <default>true</default>
    </key>
    <key name="singlefile-remove-hidden-elements" type="b">
      <default>false</default>
    </key>
    <key name="singlefile-remove-unused-styles" type="b">
      <default>true</default>
    </key>
    <key name="singlefile-remove-unused-fonts" type="b">
      <default>true</default>
    </key>
    <key name="singlefile-remove-frames" type="b">
      <default>false</default>
