    <key name="singlefile-block-images" type="b">
      <default>false</default>
    </key>
    <key name="singlefile-block-styles" type="b">
      <default>false</default>
    </key>
    <key name="singlefile-block-fonts" type="b">
      <default>false</default>
    </key>

    <key name="window-w" type="i">
      <default>600</default>
    </key>
    <key name="window-h" type="i">
      <default>700</default>
    </key>
    <key name="window-maximized" type="b">
      <default>false</default>
    </key>
  </schema>
</schemalist>
